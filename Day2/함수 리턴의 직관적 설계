#include <stdio.h>
 
int a[2][3] = { {1,2,3},{4,5,6} };
int b[2][3] = { {10,20,30},{40,50,60} };
int(*c[2])[3] = { b, a };
int(**p)[3] = c;
 
// 다음 함수의 리턴 타입을 완성하라
int (*f1(void))[3]
{
    return c[1];//*c type : int(*)[3]
}
 
void main(void)
{
    // 함수에서 복귀한 후 결과를 이용하여 a 배열에 있는 6을 인쇄하는 코드를 완성하라
    printf("6=%d\n", f1()[1][2]);//a[1][2] == f1()[1][2]
    //f1() == c[1] == a
}
